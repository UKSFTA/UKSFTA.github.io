{
  "extends": [
    "html-validate:recommended"
  ],
  "rules": {
    "text-content": "off",
    "valid-id": "off",
    "attribute-allowed-values": "off",
    "no-trailing-whitespace": "off",
    "void-content": "off",
    "no-inline-style": "off",
    "attr-quotes": "off",
    "element-required-attributes": "off",
    "no-raw-characters": "off",
    "attr-spacing": "off",
    "attr-delimiter": "off",
    "long-title": "off",
    "wcag/h37": "off",
    "wcag/h32": "off",
    "no-implicit-button-type": "off",
    "parser-error": "off",
    "no-dup-class": "off",
    "doctype-style": "off",
    "void-style": "off",
    "element-required-content": "off",
    "element-permitted-content": "off",
    "no-dup-attr": "off",
    "attr-case": "off"
  }
}